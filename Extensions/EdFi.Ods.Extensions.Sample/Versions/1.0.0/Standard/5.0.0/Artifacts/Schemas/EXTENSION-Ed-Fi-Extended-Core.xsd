<?xml version="1.0" encoding="UTF-8" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ed-fi.org/5.0.0" xmlns:ann="http://ed-fi.org/annotation" targetNamespace="http://ed-fi.org/5.0.0" elementFormDefault="qualified" attributeFormDefault="unqualified">
  <xs:include schemaLocation="Ed-Fi-Core.xsd" />
  <xs:annotation>
    <xs:documentation>===== Ed-Fi 5.0.0 Extensions =====</xs:documentation>
  </xs:annotation>
  <xs:annotation>
    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-Bus">
    <xs:annotation>
      <xs:documentation>This is an example of a new domain entity.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="BusId" type="IdentificationCode">
            <xs:annotation>
              <xs:documentation>The unique identifier for the bus.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-BusRoute">
    <xs:annotation>
      <xs:documentation>This is an example of a new domain entity.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="BusRouteNumber" type="xs:int">
            <xs:annotation>
              <xs:documentation>A unique identifier for the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusId" type="IdentificationCode">
            <xs:annotation>
              <xs:documentation>The unique identifier for the bus assigned to the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusRouteDirection" type="EXTENSION-BusRouteDirection">
            <xs:annotation>
              <xs:documentation>The direction of the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartTime" type="xs:time" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The time the bus route begins.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Daily" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An indication as to whether the bus route operates every weekday.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OperatingCost" type="Currency">
            <xs:annotation>
              <xs:documentation>The approximate annual cost for the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StartDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the bus route became operational.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="WeeklyMileage" type="EXTENSION-WeeklyMileage" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The approximate weekly mileage for the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExpectedTransitTime" type="TimeInterval">
            <xs:annotation>
              <xs:documentation>The approximate amount of time, in minutes, for the bus route operation each day.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OptimalCapacity" type="Percent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The percentage of seats filled under optimal conditions.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusYear" type="xs:gYear" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The years in which the bus route has been in active.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="HoursPerWeek" type="HoursPerWeek">
            <xs:annotation>
              <xs:documentation>The number of hours per week in which the bus route is operational.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusRouteDuration" type="Duration" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of minutes per week in which the bus route is operational.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ServiceAreaPostalCode" type="PostalCode" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The postal codes served by the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Telephone" type="Telephone" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Telephone numbers at which dispatchers may be reached for this bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Disability" type="DisabilityDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The disability served by the bus route, if applicable.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>DisabilityDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ProgramReference" type="ProgramReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Programs served by the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StaffEducationOrganizationAssignmentAssociationReference" type="StaffEducationOrganizationAssignmentAssociationReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The staff member (and associated assignment) responsible for assistance loading students on the bus.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BusReference" type="EXTENSION-BusReferenceType">
            <xs:annotation>
              <xs:documentation>The designated bus for the bus route.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ContactExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="EXTENSION-ContactRestriction">
        <xs:sequence>
          <xs:element name="IsSportsFan" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>An indication as to whether the contact is a sports fan.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CoffeeSpend" type="Currency" minOccurs="0">
            <xs:annotation>
              <xs:documentation>How much the contact spends on coffee in a week.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GraduationDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the contact graduated high school.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CeilingHeight" type="EXTENSION-CeilingHeight" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The height of the ceiling in the rooms of the contact's home.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AverageCarLineWait" type="TimeInterval" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The time spent per day waiting in the car line.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LuckyNumber" type="xs:int" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The contact's lucky number.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="RainCertainty" type="Percent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The percent likelihood that it will rain when the contact volunteers to chaperone a field trip.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FavoriteBookTitle" type="EXTENSION-FavoriteBookTitle" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The title of the contact's favorite book.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PreferredWakeUpTime" type="xs:time" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The time the contact would prefer to wake up in the morning.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BecameParent" type="xs:gYear" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The year in which the contact first became a contact.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="GPA" type="GPA" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The contact's high school GPA.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Duration" type="Duration" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The amount of time the contact spends reading to his/her children at bedtime.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Author" type="Author" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The contact's favorite authors.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CTEProgram" type="CTEProgram" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A CTE program the contact has completed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CredentialField" type="CredentialFieldDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The field in which the contact holds a credential.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>CredentialFieldDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="EducationContentReference" type="EducationContentReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Education content to which the contact has been referred.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="StudentProgramAssociationReference" type="StudentProgramAssociationReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Programs the contact's child or children are enrolled in for which the contact provides volunteer services.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Address" type="EXTENSION-AddressExtension" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Additional details on the contact's address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="TeacherConference" type="EXTENSION-TeacherConference">
            <xs:annotation>
              <xs:documentation>The contact's preferred day of the week and time for contact-teacher conferences.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-ContactRestriction">
    <xs:annotation>
      <xs:documentation>Restriction to Contact for replacement of common type with common type extension</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:restriction base="Contact">
        <xs:sequence>
          <xs:element name="ContactUniqueId" type="UniqueId">
            <xs:annotation>
              <xs:documentation>A unique alphanumeric code assigned to a contact.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Name" type="Name">
            <xs:annotation>
              <xs:documentation>Full legal name of the person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="OtherName" type="OtherName" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Other names (e.g., alias, nickname, previous legal name) associated with a person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Sex" type="SexDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A person's birth sex.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>SexDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="GenderIdentity" type="GenderIdentity" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The gender the contact identifies themselves as.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InternationalAddress" type="InternationalAddress" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The set of elements that describes an international address.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Telephone" type="Telephone" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The 10-digit telephone number, including the area code, for the person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ElectronicMail" type="ElectronicMail" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The numbers, letters, and symbols used to identify an electronic mail (e-mail) user within the network to which the individual or organization belongs.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LoginId" type="IdentificationCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The login ID for the user; used for security access control interface.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Language" type="Language" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The language(s) the individual uses to communicate. It is strongly recommended that entries use only ISO 639-2 language codes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PersonReference" type="PersonReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Relates the contact to a generic person.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="HighestCompletedLevelOfEducation" type="LevelOfEducationDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The extent of formal instruction an individual has received (e.g., the highest grade in school completed or its equivalent or the highest degree received).</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>LevelOfEducationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:restriction>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SchoolExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="School">
        <xs:sequence>
          <xs:element name="IsExemplary" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>An indication as to whether the school is exemplary.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CTEProgram" type="CTEProgram" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A CTE program the school is known for.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="DirectlyOwnedBusReference" type="EXTENSION-BusReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Buses owned by the School directly.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StaffExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Staff">
        <xs:sequence>
          <xs:element name="Pet" type="EXTENSION-Pet" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Details about pets in the staff member's household.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FirstPetOwnedDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The date the staff member adopted the first household pet.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PetPreference" type="EXTENSION-PetPreference" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Details about the staff member's pet preferences.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Student">
        <xs:sequence>
          <xs:element name="Pet" type="EXTENSION-Pet" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Details about pets in the student's household.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PetPreference" type="EXTENSION-PetPreference" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Details about the student's pet preferences.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="AquaticPet" type="EXTENSION-AquaticPet" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Details about aquatic pets in the student's household</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="FavoriteBook" type="EXTENSION-FavoriteBook" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>This is documentation.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Descriptors =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-ArtMediumDescriptor">
    <xs:annotation>
      <xs:documentation>Art medium used in a section or program (i.e. paint, pencils, clay, etc.).</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-FavoriteBookCategoryDescriptor">
    <xs:annotation>
      <xs:documentation>The category of an individual's favorite book.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-MembershipTypeDescriptor">
    <xs:annotation>
      <xs:documentation>Membership Type identifies whether a school has primary responsibility for managing a specific student's curriculum or not.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Descriptor</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DescriptorType" />
    </xs:complexContent>
  </xs:complexType>
  <xs:annotation>
    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentArtProgramAssociation">
    <xs:annotation>
      <xs:documentation>This is an example of a subclass of an association.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="GeneralStudentProgramAssociation">
        <xs:sequence>
          <xs:element name="IdentificationCode" type="IdentificationCode" minOccurs="0">
            <xs:annotation>
              <xs:documentation>A unique identification code used to identify the student's artwork produced in the program.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ExhibitDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Start date for the program's art exhibit to display the student's work.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ArtMedium" type="EXTENSION-ArtMediumDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The art mediums used in the program (i.e., paint, pencils, clay, etc.).</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-ArtMediumDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="ProgramFees" type="Currency" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Required program fees to purchase materials for the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Service" type="Service" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Indicates the Service(s) being provided to the Student by the Program.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="NumberOfDaysInAttendance" type="NumberOfDaysInAttendance" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Number of days the student is in attendance at the program.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="HoursPerDay" type="EXTENSION-HoursPerDay" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of hours a student spends in the program each school day.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PrivateArtProgram" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Indicator that the student participated in art education at a private agency or institution.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="KilnReservation" type="xs:time" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The student's reserved time for use of the kiln.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="KilnReservationLength" type="TimeInterval" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The number of clock minutes dedicated to the student's kiln reservation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="Style" type="EXTENSION-Style" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The art style(s) exhibited by the student in the program.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ArtPieces" type="EXTENSION-ArtPieces" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The total number of art pieces completed by the student during the program.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PortfolioYears" type="xs:gYear" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>The of year(s) of work included in the student's portfolio.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PortfolioPieces" type="EXTENSION-PortfolioPieces" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The total number of pieces in the student's portfolio.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="MasteredMediums" type="Percent" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Percentage of the mediums taught in the program which the student mastered.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentContactAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentContactAssociation">
        <xs:sequence>
          <xs:element name="PriorContactRestrictions" type="ContactRestrictions" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Previous restrictions for student and/or teacher contact with the individual (e.g., the student may not be picked up by the individual).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BedtimeReader" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>An indication as to whether the contact regularly reads to the student before bed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="BookBudget" type="Currency" minOccurs="0">
            <xs:annotation>
              <xs:documentation>The contact's estimated monthly budget dedicated to books for the student.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReadGreenEggsAndHamDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Date on which the contact first read the student Green Eggs and Ham by Dr. Seuss.</xs:documentation>
            </xs:annotation>
          </xs:element>
