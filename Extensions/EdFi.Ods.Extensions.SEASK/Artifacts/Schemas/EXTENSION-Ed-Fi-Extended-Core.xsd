    <xs:documentation>===== Associations =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentDisciplineIncidentAssociationExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Association</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="StudentDisciplineIncidentAssociation">
        <xs:sequence>
          <xs:element name="Weapon" type="WeaponDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Identifies the type of weapon used during an incident.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>WeaponDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="GunFreeSchoolViolation" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>True = Student was charged with a Gun-Free Schools Violation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolRelatedArrestIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if student was involved in a disciplinary incident that resulted in an arrest.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ReferralToLawEnforcement" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if a student was referred to law enforcement.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SeriousBodilyInjury" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if an incident resulted in serious bodily injury as defined by 18 U.S.C. Section 1365(3)(h).</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-StudentEarlyLearningProgramAssociation">
