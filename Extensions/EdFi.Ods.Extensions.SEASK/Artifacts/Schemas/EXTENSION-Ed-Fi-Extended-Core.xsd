    <xs:documentation>===== Domain Entities =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-CalendarDateExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="CalendarDate">
        <xs:sequence>
          <xs:element name="InstructionalDuration" type="EventDuration">
            <xs:annotation>
              <xs:documentation>The portion of the day that instruction occurred.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-CrisisDetails">
    <xs:annotation>
      <xs:documentation>Information about the specific crisis that occurred.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="CrisisEvent" type="EXTENSION-CrisisEventDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The specific event that occurred</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CrisisEventDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CrisisType" type="EXTENSION-CrisisTypeDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>The type of crisis event that occurred</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-CrisisTypeDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="CrisisStartDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>First date that the crisis occurred</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="CrisisDesription" type="EXTENSION-CrisisDesription" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Description of the event</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineActionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineAction">
        <xs:sequence>
          <xs:element name="UnilateralRemoval" type="EXTENSION-UnilateralRemovalDescriptorReferenceType" minOccurs="0">
            <xs:annotation>
              <xs:documentation>For SPED students only who were were subject to Uniliteral Removal</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-UnilateralRemovalDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="GFSAExpulsionModifiedToLessThanOneYear" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>True = Student expelled under GFSA had expulsion modified to less than one year.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-DisciplineIncidentExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="DisciplineIncident">
        <xs:sequence>
          <xs:element name="HomicideIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if this discipline incident involved homicide.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="ShootingIndicator" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Indicates if the discipline incident involved a shooting.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-PostGraduateActivity">
    <xs:annotation>
      <xs:documentation>NDE new Domain for capturing the CTE post graduation activity details. The Post School Survey template is provided for all 12th grade Career Education Concentrators identified as a [School-Year] completer with diploma and is submitted ONE-YEAR AFTER high school graduation.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="ComplexObjectType">
        <xs:sequence>
          <xs:element name="StudentReference" type="StudentReferenceType">
            <xs:annotation>
              <xs:documentation>The NDE student identifier (10-digit) for the CTE Post graduate student. This cannot be a student reference since the Post Graduate Activity is reported one year after student's graduation and student will not be in current year student list.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SchoolYear" type="SchoolYearType">
            <xs:annotation>
              <xs:documentation>Represents the school year during which the student graduated.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="LocalEducationAgencyReference" type="LocalEducationAgencyReferenceType">
            <xs:annotation>
              <xs:documentation>The number assigned to your school district/system by NDE.  The district that submitted the graduation record for the CTE Student graduate.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="PostGraduateActivity" type="EXTENSION-PostGraduateActivityDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Appropriate code to describe the student's activity after completion.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-PostGraduateActivityDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
          <xs:element name="PostGraduateActivityDetermination" type="EXTENSION-PostGraduateActivityDeterminationDescriptorReferenceType">
            <xs:annotation>
              <xs:documentation>Appropriate code to describe the student's activity after completion.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-PostGraduateActivityDeterminationDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="EXTENSION-SectionExtension">
    <xs:annotation>
      <xs:documentation></xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Domain Entity</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="Section">
        <xs:sequence>
          <xs:element name="CourseStaffIdOverride" type="EXTENSION-CourseStaffIdOverride" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Staff Id of non district employee correspoinding to the teacher of the course.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SectionDelivery" type="EXTENSION-SectionDeliveryDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded">
            <xs:annotation>
              <xs:documentation>Description of the delivery method for a section of a course.</xs:documentation>
              <xs:appinfo>
                <ann:Descriptor>EXTENSION-SectionDeliveryDescriptor</ann:Descriptor>
              </xs:appinfo>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
