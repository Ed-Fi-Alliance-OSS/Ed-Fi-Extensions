        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-DualCreditDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>To indicate if student earned both high school and postsecondary credit for this course.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-EarlyLearningSettingDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>To indicate the classroom setting of the student's program participation.</xs:documentation>
