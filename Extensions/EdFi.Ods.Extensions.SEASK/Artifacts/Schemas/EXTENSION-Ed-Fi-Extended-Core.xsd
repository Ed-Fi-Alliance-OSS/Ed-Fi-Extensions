  <xs:annotation>
    <xs:documentation>===== Common Types =====</xs:documentation>
  </xs:annotation>
  <xs:complexType name="EXTENSION-StudentCrisisEvent">
    <xs:annotation>
      <xs:documentation>Information on the crisis that has affected a student</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Common</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="CrisisEvent" type="EXTENSION-CrisisEventDescriptorReferenceType">
        <xs:annotation>
          <xs:documentation>The specific event that affected the student</xs:documentation>
          <xs:appinfo>
            <ann:Descriptor>EXTENSION-CrisisEventDescriptor</ann:Descriptor>
          </xs:appinfo>
        </xs:annotation>
      </xs:element>
      <xs:element name="DisplacedStudentIndicator" type="xs:boolean">
        <xs:annotation>
          <xs:documentation>An indication that the student was displaced from their home due to the crisis. The student may or may not be considered homeless due to the displacement.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
