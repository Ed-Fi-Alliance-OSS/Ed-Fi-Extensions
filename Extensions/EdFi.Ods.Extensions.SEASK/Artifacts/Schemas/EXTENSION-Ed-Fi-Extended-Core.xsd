        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-ImmigrantIndicatorDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>Description that best reflects the student's immigrant status.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-PlacementTypeDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>Indicates placement type of the student.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-PositionTitleDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>Description of the default position titles for the dashboard claimset mapping.</xs:documentation>
      <xs:appinfo>
        <ann:TypeGroup>Extended Descriptor Reference</ann:TypeGroup>
      </xs:appinfo>
    </xs:annotation>
    <xs:restriction base="DescriptorReferenceType" />
  </xs:simpleType>
  <xs:simpleType name="EXTENSION-PostGraduateActivityDescriptorReferenceType">
    <xs:annotation>
      <xs:documentation>Description of the student�s activity after completion.</xs:documentation>
