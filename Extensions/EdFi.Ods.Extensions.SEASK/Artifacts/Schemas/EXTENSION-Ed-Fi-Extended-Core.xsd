            </xs:annotation>
          </xs:element>
          <xs:element name="ToTakeAlternateAssessment" type="xs:boolean" minOccurs="0">
            <xs:annotation>
              <xs:documentation>True = Indicates the student needs alternate assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InitialSpecialEducationEntryDate" type="xs:date" minOccurs="0">
            <xs:annotation>
              <xs:documentation>Date when the child's first IEP or IFSP</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
